on id=\"X\" onclick=\"appendToDisplay('*')\" class=\"operator-btn\">×</button>\n\n\n            <button id=\"1/x\" onclick=\"appendToDisplay('1/')\" class=\"sci-op\">1/x</button>\n            <button id=\"2rx\" onclick=\"sqrt()\" class=\"sci-op\">²√x</button>\n            <button id=\"3rx\" onclick=\"cbrt()\" class=\"sci-op\">³√x</button>\n            <button id=\"yrx\" onclick=\"yrt()\" class=\"sci-op\">ʸ√x</button>\n            <button id=\"ln\" onclick=\"natlog()\" class=\"sci-op\">ln</button>\n            <button id=\"log10\" onclick=\"logaritm()\" class=\"sci-op\">log₁₀</button>\n            <button id=\"4\" onclick=\"appendToDisplay('4')\">4</button>\n            <button id=\"5\" onclick=\"appendToDisplay('5')\">5</button>\n            <button id=\"6\" onclick=\"appendToDisplay('6')\">6</button>\n            <button id=\"-\" onclick=\"appendToDisplay('-')\" class=\"operator-btn\">—</button>\n\n\n            <button id=\"x!\" onclick=\"ten_pow()\" class=\"sci-op\">x!</button>\n            <button id=\"sin\" onclick=\"sine()\" class=\"sci-op\">sin</button>\n            <button id=\"cos\" onclick=\"cosine()\" class=\"sci-op\">cos</button>\n            <button id=\"tan\" onclick=\"tangent()\" class=\"sci-op\">tan</button>\n            <button id=\"e\" onclick=\"appendToDisplay('Math.E')\" class=\"sci-op\">e</button>\n            <button id=\"EE\" onclick=\"appendToDisplay('e')\" class=\"sci-op\">EE</button>\n            <button id=\"1\" onclick=\"appendToDisplay('1')\">1</button>\n            <button id=\"2\" onclick=\"appendToDisplay('2')\">2</button>\n            <button id=\"3\" onclick=\"appendToDisplay('3')\">3</button>\n            <button id=\"+\" onclick=\"appendToDisplay('+')\" class=\"operator-btn\">＋</button>\n\n\n            <button id=\"toggle-sci\" onclick=\"toggleScientific()\" class=\"sci-op\">⏼</button>\n            <button id=\"sinh\" onclick=\"hsine()\" class=\"sci-op\">sinh</button>\n            <button id=\"cosh\" onclick=\"hcosine()\" class=\"sci-op\">cosh</button>\n            <button id=\"tanh\" onclick=\"htangent()\" class=\"sci-op\">tanh</button>\n            <button id=\"pi\" onclick=\"appendToDisplay('Math.PI')\" class=\"sci-op\">π</button>\n            <button id=\"radian\" onclick=\"toggleRadiansDegrees()\" class=\"sci-op\">Rad</button>\n            <button id=\"00\" onclick=\"appendToDisplay('00')\">00</button>\n            <button id=\"0\" onclick=\"appendToDisplay('0')\">0</button>\n            <button id=\".\" onclick=\"appendToDisplay('.')\">.</button>\n            <button id=\"=\" onclick=\"calculate()\" class=\"operator-btn\">=</button>\n        </div>\n    </div>\n\n    <script src=\"index.js\"></script>\n</body>\n</html>",
